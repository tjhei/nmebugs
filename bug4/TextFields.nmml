<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<app title="TextFields" file="TextFields" main="TextFields" package="org.haxenme.tutorial.textfields" version="1.0.0" company="NME" />
	
	<set name="fullscreen" if="target_webos" />
	<set name="fullscreen" if="target_ios" />
	<set name="fullscreen" if="target_android" />
	
	<window width="0" height="0" fps="30" orientation="portrait" resizable="true" />
 	
